<?php
require 'dbgp/dbgpclient.php';
$filename = dirname(__FILE__) . '/bug00790-002.inc';

$commands = array(
	'stdout -c 1',
	'run',
	'run',
	'detach',
);

dbgpRunFile( $filename, $commands );
?>
